<script type="text/javascript">
    var noww = new Date();
    var day = "0" + noww.getDate();
    var month = "0" + (noww.getMonth()+1);
    Mackolik.Program.sport = 1;
    Mackolik.Program.type = Mackolik.Program.sport == 1 ? 6 : 1;
    Mackolik.Program.sort = "-1";
    Mackolik.Program.sortDir = "-1";
    Mackolik.Program.sortValue = "DATE";
    Mackolik.Program.groupId = "-1";
    Mackolik.Program.day = day.substr(day.length - 2, 2) + "." + month.substr(month.length - 2, 2) + "." + noww.getFullYear();
    Mackolik.Program.currentWeek = "24105";
    Mackolik.Program.week = "24105";
    Mackolik.Program.notPlayed = "1";
    Mackolik.Program.Large.getProgram();
        
    window.onscroll = scroll;
    window.onresize=  resize;

    function resize(){
        headerAlign();
    }

   function scroll()
   {
       isIE6 = navigator.userAgent.toLowerCase().indexOf('msie 6') != -1;

       if (isIE6) {
           document.getElementById("dvLargeHead").style.position = "absolute";
           document.getElementById("dvLargeHead").style.top = document.documentElement.scrollTop;
           document.getElementById("dvLargeHead").style.opacity = 100;
       } else{
           document.getElementById("dvLargeHead").style.top = "0px";
           document.getElementById("dvLargeHead").style.left = -1 * $(document).scrollLeft() + "px";
       }

       
       if (Math.max(document.documentElement.scrollTop, document.body.scrollTop) > 220) {
           document.getElementById("dvLargeHead").style.display = "block";
       }
       else
           document.getElementById("dvLargeHead").style.display = "none";

    }

    //Mackolik.Coupon.initializeCouponFace("overflowcoupon", null, 0, false, 140, 740);
</script>
