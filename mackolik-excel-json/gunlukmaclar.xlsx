    <script type="text/javascript">
     
    function changeSubGroup(groupId)
	{
	    Mackolik.Program.groupId = groupId;
	    Mackolik.Program.sort="-1";
	    Mackolik.Program.sortDir="1";
	    Mackolik.Program.Large.getProgram();
	}
	
	function changeDay(day)
	{
	    Mackolik.Program.day=day;
	    Mackolik.Program.groupId="-1";
	    Mackolik.Program.sort="-1";
	    Mackolik.Program.sortDir="1";
	    Mackolik.Program.Large.getProgram();
	    
	}
	
	function changeWeek(weekId)
	{
	    Mackolik.Program.week=weekId;
    	Mackolik.Program.day="-1";
	    Mackolik.Program.groupId="-1";
	    Mackolik.Program.sort="-1";
	    Mackolik.Program.sortDir="1";
        
        if (Mackolik.Program.week*1 < Mackolik.Program.currentWeek*1) {
            Mackolik.Program.notPlayed = "0";
            $("#justNotPlayed").attr("checked",false);
        }

	    Mackolik.Program.Large.getProgram();
	}

	function JustNotPlayedClicked(obj) {
	    Mackolik.Program.notPlayed = obj.checked ? "1" : "0";
	    Mackolik.Program.Large.getProgram();
    }

	function replaceTurkish(str){
	    var str2=str;
	    str2=str2.replace("Ç",'C');
	    str2=str2.replace("Ğ",'G');
	    str2=str2.replace("İ",'I');
	    str2=str2.replace("Ü",'U');
	    str2=str2.replace("Ö",'O');
	    str2=str2.replace("Ş",'S');
	    return str2;
	}
    function addColor(imgSort,sort)
    {
        if(sort==1)
            imgSort.src = "//im.mackolik.com/img5/icons/sort-orange-top.gif";
        else
            imgSort.src = "//im.mackolik.com/img5/icons/sort-orange-bottom.gif";
    }
    
    function removeColor(imgSort,sort)
    {
        if(sort==1)
            imgSort.src = "//im.mackolik.com/img5/icons/sort-blue-top.gif";
        else
            imgSort.src = "//im.mackolik.com/img5/icons/sort-blue-bottom.gif";  
    }
    
    var TEMPTROBJCOLOR ;
    var TEMPTROBJ;
    function HLon(obj)
    {
	    if (TEMPTROBJ != null)
	    {
		    TEMPTROBJ.style.backgroundColor = TEMPTROBJCOLOR; 
	    } 
	    TEMPTROBJ = obj;
	    TEMPTROBJCOLOR = obj.style.backgroundColor;
	    obj.style.backgroundColor = '#FFE57E';
    }

    function HLoff()
    {	
	    if (TEMPTROBJ != null)
	    {
		    TEMPTROBJ.style.backgroundColor = TEMPTROBJCOLOR; 
	    }	
    }

